package cn.itcast.bean;

public class Person {

	private int age;
	private String name;
	
	public Person(int age, String name) {
		super();
		this.age = age;
		this.name = name;
	}
	public Person() {
		super();
		
	}
	
	public void show(){
		System.out.println(name+"...show run..."+age);
	}
}
